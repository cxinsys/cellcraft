rule dataTable:
    input:
        "workflow/data/build_{filename}.txt",
        "workflow/data/{filename}.csv"
    output:
        "workflow/data/DataTable_{filename}.csv",
        "workflow/result/DataTable_{filename}.html"
    script:
        "script/dataTable.py"

rule scatterPlot:
    input:
        "workflow/data/build_{filename}.txt",
        "workflow/data/DataTable_{filename}.csv"
    output:
        "workflow/data/ScatterPlot_{filename}.csv",
        "workflow/result/ScatterPlot_{filename}.png"
    script:
        "script/scatterPlot.py"